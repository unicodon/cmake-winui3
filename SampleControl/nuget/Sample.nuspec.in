<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
	<metadata>
		<id>Sample</id>
		<version>@PACKAGE_VERSION@</version>
		<authors>Author</authors>
		<description>A simple component</description>
		<dependencies>
			<group targetFramework="net6.0-windows10.0.18362.0" />
			<group targetFramework="UAP10.0" />
		</dependencies>
	</metadata>
	<files>
		<!--Architecture-netural assemblies-->
		<file src="@CMAKE_CURRENT_BINARY_DIR@\Sample.Projection.dll" target="lib\net6.0-windows10.0.18362.0\" />
		<file src="@CMAKE_CURRENT_BINARY_DIR@\Sample.winmd" target="lib\uap10.0\Sample.winmd" />
		<!--Architecture-specific implementation DLLs should be copied into RID-relative folders-->
		<file src="@CMAKE_CURRENT_BINARY_DIR@\Sample.dll" target="runtimes\win10-x64\native\Sample.dll" />
	</files>
</package>
